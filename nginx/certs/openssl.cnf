[ req ]
default_bits = 2048
default_keyfile = privkey.pem
distinguished_name = req_distinguished_name
req_extensions = req_ext
x509_extensions = v3_req

[ req_distinguished_name ]
countryName_default = BG
stateOrProvinceName_default = Burgas
localityName_default = Kameno
organizationName_default = HomeServer
commonName_default = homeserver.lan

[ req_ext ]
subjectAltName = @alt_names

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = homeserver.lan
DNS.2 = localhost
IP.1 = 127.0.0.1
IP.2 = 192.168.1.100
